{
  "name": "tooloxide",
  "description": "A collection of blazingly fast tools. SvelteKit + Rust <3",
  "author": "Hongmeng Zhai <zhaihongmeng@gmail.com>",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=22.11.0"
  },
  "scripts": {
    "dev": "bun build:rust && bunx vercel develop",
    "develop": "vite dev",
    "build": "bun build:rust && vite build",
    "build:api": "cargo build -r",
    "build:wasm": "./build_wasm.sh",
    "build:rust": "bun build:api && bun build:wasm",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:e2e": "playwright test",
    "test": "npm run test:e2e",
    "prepare": "husky"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.11.3",
    "@inlang/paraglide-sveltekit": "0.15.0",
    "@playwright/test": "1.49.1",
    "@sveltejs/adapter-vercel": "^5.5.2",
    "@sveltejs/kit": "2.10.1",
    "@sveltejs/vite-plugin-svelte": "5.0.1",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "autoprefixer": "10.4.20",
    "bits-ui": "^1.0.0-next.72",
    "clsx": "^2.1.1",
    "daisyui": "^4.12.22",
    "eslint": "^9.17.0",
    "eslint-plugin-svelte": "^2.46.1",
    "husky": "^9.1.7",
    "lucide-svelte": "^0.468.0",
    "mdsvex": "0.12.3",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "5.10.0",
    "svelte-check": "4.1.1",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "twglow": "^0.0.10",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vercel": "^39.2.2"
  },
  "dependencies": {
    "@fontsource-variable/space-grotesk": "^5.1.0",
    "@fontsource/space-mono": "^5.1.1",
    "carta-md": "^4.6.7"
  }
}
